
<ReactionScheme>
<!-- First version of merges Gq Gs coupled pathway reactions
     when reactions differ between the models, notes indicate which was source
     Either StriatumCRCNS/StochasticSP1/MSPNfinalxml/MSPNreactions_4spine_new50.xml for Gs
     or MURI/BoHung/FinalMarch2012/mglu_2ag_reac12mar20desens025.xml for Gq
phosphatase reactions take from revision of Gs to improve Ca dep dphos of Dp32-->

<!-- Molecules species and diffusion constants -->


<Specie name="Cof" id="Cof" kdiff="5.77" kdiffunit="mu2/s"  />
<Specie name="pCof" id="pCof"  kdiff="22.91" kdiffunit="mu2/s"/>
<Specie name="pLIMKCof" id="pLIMKCof"  kdiff="0" kdiffunit="mu2/s"/>
<Specie name="LIMK" id="LIMK" kdiff="6.7" kdiffunit="mu2/s"/>


<Specie name="pLIMK" id="pLIMK" kdiff="14.50" kdiffunit="mu2/s"   />
<Specie name="RacPAKLIMK" id="RacPAKLIMK" kdiff="0" kdiffunit="mu2/s"/>
<Specie name="PAK" id="PAK" kdiff="15.53" kdiffunit="mu2/s"/>
<Specie name="RacPAK" id="RacPAK" kdiff="7.1" kdiffunit="mu2/s"/>
<Specie name="RacGTP" id="RacGTP" kdiff="22.15"  kdiffunit="mu2/s"/>
<Specie name="RacGDP" id="RacGDP" kdiff="0" kdiffunit="mu2/s"/>
<Specie name="RacGap" id="RacGap" kdiff="0"  kdiffunit="mu2/s"/>
<Specie name="RacPAKGap" id="RacPAKGap" kdiff="0"  kdiffunit="mu2/s"/>
<Specie name="Gap" id="Gap" kdiff="6.8"  kdiffunit="mu2/s"/>

<Specie name="pKalRac" id="pKalRac" kdiff="10.88" kdiffunit="mu2/s"/>

<Specie name="pKal" id="pKal" kdiff="10.88"  kdiffunit="mu2/s"/>
<Specie name="pKalPP1" id="pKalPP1" kdiff="0" kdiffunit="mu2/s"/> <!-- is that correct -->
<Specie name="KalCKpCamCa4" id="KalCKpCamCa4" kdiff="0" kdiffunit="mu2/s"/>
<Specie name="KalCKCamCa4" id="KalCKCamCa4" kdiff="0" kdiffunit="mu2/s"/>
<Specie name="Kal" id="Kal" kdiff="10.88" kdiffunit="mu2/s"/>



<Specie name="CaNCamCa4"        id="CaNCamCa4"       kdiff="0"     kdiffunit="mu2/s"/>
<Specie name="CaNCamCa4pSSH"        id="CaNCamCa4pSSH"       kdiff="0"     kdiffunit="mu2/s"/>
<Specie name="pSSH"        id="pSSH"       kdiff="0"     kdiffunit="mu2/s"/>
<Specie name="SSH" id="SSH" kdiff="5.6" kdiffunit="mu2/s"/>
<Specie name="SSHpLIMK" id="SSHpLIMK" kdiff="0"  kdiffunit="mu2/s"/>
<Specie name="SSHpCof" id="SSHpCof" kdiff="0" kdiffunit="mu2/s"/>
<Specie name="RacPAKSSH" id="RacPAKSSH" kdiff="0" kdiffunit="mu2/s"/>



    <Specie name="CKCamCa4"                            id="CKCamCa4"              kdiff="0.01"     kdiffunit="mu2/s"/>
    <Specie name="CKpCamCa4"                           id="CKpCamCa4"             kdiff="0.01"     kdiffunit="mu2/s"/>
<Specie name="PP1"                                 id="PP1"               kdiff="0"    kdiffunit="mu2/s"/>
    
   
<!--****************************************************************************
Now, the reactions themselves
****************************************************************************-->
<!--activation of Kal-->
<!--CamKII to Kal INCREASE RATE LATER BY ABOUT 20% -->
<!--CamKII+Kal<->CamKII +pKal-->
<Reaction id="Kal+CKpCamCa4--KalCKpCamCa4" name="Kal+CKpCamCa4--KalCKpCamCa4">
<Reactant specieID="Kal"/>
<Reactant specieID="CKpCamCa4"/>
<Product specieID="KalCKpCamCa4"/>

<!--forwardRate> 0</forwardRate-->
<forwardRate> 123.5e-9 </forwardRate>
<reverseRate> 8e-3 </reverseRate>

</Reaction>
<Reaction id="KalCKpCamCa4--pKal+CKpCamCa4" name="KalCKpCamCa4--pKal+CKpCamCa4">
<Reactant specieID="KalCKpCamCa4"/>
<Product specieID="CKpCamCa4"/>
<Product specieID="pKal"/>

<forwardRate> 2e-3 </forwardRate>
<reverseRate> 0 </reverseRate>

</Reaction>


<!--CamKII to Kal -->
<!--CamKII+Kal<->CamKII +pKal-->
<Reaction id="Kal+CKCamCa4--KalCKCamCa4" name="Kal+CKCamCa4--KalCKCamCa4">
<Reactant specieID="Kal"/>
<Reactant specieID="CKCamCa4"/>
<Product specieID="KalCKCamCa4"/>

<!--forwardRate> 0</forwardRate-->
<forwardRate> 61.7e-9 </forwardRate>
<reverseRate> 0.8e-3 </reverseRate>

</Reaction>
<Reaction id="KalCKCamCa4--pKal+CKCamCa4" name="KalCKCamCa4--pKal+CKCamCa4">
<Reactant specieID="KalCKCamCa4"/>
<Product specieID="CKCamCa4"/>
<Product specieID="pKal"/>

<forwardRate> 0.2e-3 </forwardRate>
<reverseRate> 0 </reverseRate>

</Reaction>

 

<!--pKal inactivation -->
<!--pKal<->Kal-->
<Reaction id="pKal--Kal" name="pKal--Kal">
<Reactant specieID="pKal"/>
<Product specieID="Kal"/>

<!--forwardRate> 0</forwardRate-->
<forwardRate> 0.01e-3 </forwardRate>
<reverseRate> 0</reverseRate>

</Reaction>

<!--pKal inactivation with PP1 -->
<!--pKal+PP1<->Kal+PP1-->
<Reaction id="pKal+PP1--Kal+PP1" name="pKal+PP1--Kal+PP1">
  <Reactant specieID="pKal"/>
  <Reactant specieID="PP1"/>
  <Product specieID="pKalPP1"/>
  
<!--forwardRate> 0</forwardRate-->
<forwardRate> 0.00005e-3 </forwardRate>
<reverseRate> 0.04e-3</reverseRate>

</Reaction>

<Reaction id="pKal+PP1--Kal+PP1" name="pKal+PP1--Kal+PP1">
  <Reactant specieID="pKalPP1"/>
  <Product specieID="PP1"/>
  <Product specieID="Kal"/>

<!--forwardRate> 0</forwardRate-->
<forwardRate> 0.01e-3 </forwardRate>
<reverseRate> 0</reverseRate>

</Reaction>

<!--RacGTP activation -->
<!--RacGDP+ pKal<->RacGTP + pKal-->
<Reaction id="RacGDP+Kal--RacGDPpKal" name="RacGDP+pKal--pKalRacGDP">
<Reactant specieID="pKal"/>
<Reactant specieID="RacGDP"/>
<Product specieID="pKalRac"/>

<!--forwardRate> 0</forwardRate-->
<forwardRate> 0.003703704e-3 </forwardRate>
<reverseRate> 4e-3 </reverseRate>

</Reaction>

<Reaction id="RacGDP+pKal--RacGTP+pKal" name="RacGDP+pKal--pKal+RacGTP">
<Reactant specieID="pKalRac"/>
<Product specieID="RacGTP"/>
<Product specieID="pKal"/>

<forwardRate> 1e-3 </forwardRate>
<reverseRate> 0 </reverseRate>

</Reaction>


<!--RacGTP inactivation -->
<!--RacGTP+ Gap<->RacGDP + Gap-->
<Reaction id="RacGTP+Gap--RacGTPGap" name="RacGTP+Gap--GapRacGTP">
<Reactant specieID="Gap"/>
<Reactant specieID="RacGTP"/>
<Product specieID="RacGap"/>

<!--forwardRate> 0</forwardRate-->
<forwardRate> 0.00022e-3 </forwardRate>
<reverseRate> 4.82e-3</reverseRate>

</Reaction>

<Reaction id="RacGTP+Gap--RacGTPGap" name="RacGTP+Gap--Gap+RacGDP">
<Reactant specieID="RacGap"/>
<Product specieID="RacGDP"/>
<Product specieID="Gap"/>

<forwardRate> 1.21e-3 </forwardRate>
<reverseRate> 0e-3 </reverseRate>

</Reaction>

<!--RacGTP intrinsic inactivation -->
<!--RacGTP<->RacGDP-->
<Reaction id="RacGTP--RacGTP" name="RacGTP--RacGTP">
<Reactant specieID="RacGTP"/>
<Product specieID="RacGDP"/>

<!--forwardRate> 0</forwardRate-->
<forwardRate> 0.000009e-3 </forwardRate>
<reverseRate> 0</reverseRate>

</Reaction>

<!--RacGTP activate PAK-->
<!--RacGTP+PAK <->RacPAK-->
<Reaction id="RacGTP+PAK--RacGTPPAK" name="PAK+RacGTP--RacGTPPAK">
<Reactant specieID="PAK"/>
<Reactant specieID="RacGTP"/>
<Product specieID="RacPAK"/>


<!--forwardRate> 0</forwardRate-->
<forwardRate> 0.0096e-3 </forwardRate>
<reverseRate> 1.5e-3 </reverseRate>

</Reaction>




<!--LIMK activated by PAK-->
<!--PAK+LIMK <->pLIMK+PAK-->
<Reaction id="RacPAK+LIMK--LIMKRacPAK" name="RacPAK+LIMK--LIMKRacPAK">
<Reactant specieID="RacPAK"/>
<Reactant specieID="LIMK"/>
<Product specieID="RacPAKLIMK"/>

<!--forwardRate> 0</forwardRate-->
<forwardRate> 0.000102394e-3 </forwardRate>
<reverseRate> 0.00585221e-3 </reverseRate>

</Reaction>


<Reaction id="RacPAK+LIMK--RacPAK+pLIMK" name="RacPAK+LIMK--pLIMK+RacPAK">
<Reactant specieID="RacPAKLIMK"/>
<Product specieID="pLIMK"/>
<Product specieID="RacPAK"/>

<forwardRate> 0.00109424e-3 </forwardRate>
<reverseRate> 0 </reverseRate>

</Reaction>





<!--LIMK inactivation -->
<!--pLIMK+LIMK <->LIMK-->
<Reaction id="pLIMK--LIMK" name="pLIMK--LIMK">
<Reactant specieID="pLIMK"/>
<Product specieID="LIMK"/>

<!--forwardRate> 0</forwardRate-->
<forwardRate> 0.01e-3 </forwardRate>
<reverseRate> 0 </reverseRate>

</Reaction>



<!-- CaN activate SSH-->
<!--  CaNCam+pSSH <->CaNCam+SSH       -->
<Reaction name = "CaNCamCa4+pSSH--SSH" id="CaNCamCa4+pSSH--SSH">
        <Reactant specieID="pSSH"                    />
        <Reactant specieID="CaNCamCa4"                   />
        <Product  specieID="CaNCamCa4pSSH"                />

        <forwardRate> 6.21711e-09 </forwardRate>
        <reverseRate> 0.0124667e-3 </reverseRate>
</Reaction>

<!--  CaNCam+pSSH <->CaNCam+SSH       -->
<Reaction name = "CaNCam+pSSH--SSH" id="CaNCam+pSSH--SSH">
        <Reactant specieID="CaNCamCa4pSSH"                    />
        <Product  specieID="CaNCamCa4"                />
        <Product  specieID="SSH"                />
	
        <forwardRate> 0.000240287e-3 </forwardRate>
        <reverseRate> 0 </reverseRate>
</Reaction>

<!--PAK inactivate SSH through phosphorylation-->
<Reaction id="RacPAK+SSH--pSSH+RacPAK" name="RacPAK+SSH--pSSH+RacPAK">
<Reactant specieID="RacPAK"/>
<Reactant specieID="SSH"/>
<Product specieID="RacPAKSSH"/>

<!--forwardRate> 0</forwardRate-->
<forwardRate> 0.105862e-3 </forwardRate>
<reverseRate> 0.49049e-3 </reverseRate>
</Reaction>

<Reaction id="RacPAK+SSH--pSSH+RacPAK" name="RacPAK+SSH--pSSH+RacPAK">
<Reactant specieID="RacPAKSSH"/>
<Product specieID="pSSH"/>
<Product specieID="RacPAK"/>

<forwardRate> 0.0119511e-3 </forwardRate>
<reverseRate> 0 </reverseRate>

</Reaction>



<!--LIMK inactivation Cof-->
<!--Cof+pLIMK <->pLIMK+pCof-->
<Reaction id="Cof+pLIMK--pLIMKCof" name="Cof+pLIMK--pLIMKCof">
<Reactant specieID="Cof"/>
<Reactant specieID="pLIMK"/>
<Product specieID="pLIMKCof"/>

<!--forwardRate> 0</forwardRate-->
<forwardRate> 5.29618e-09 </forwardRate>
<reverseRate> 0.000302187e-3 </reverseRate>

</Reaction>

<Reaction id="Cof+pLIMK--pCof+pLIMK" name="Cof+pLIMK--pLIMK+pCof">
<Reactant specieID="pLIMKCof"/>
<Product specieID="pLIMK"/>
<Product specieID="pCof"/>

<forwardRate> 1.59464e-09</forwardRate>
<reverseRate> 0 </reverseRate>
</Reaction>


<!--SSH inactivation LIMK-->
<!--pLIMK+SSH <->SSH+LIMK-->
<Reaction id="pLIMK+SSH--SSHpLIMK" name="pLIMK+SSH--SSHpLIMK">
<Reactant specieID="pLIMK"/>
<Reactant specieID="SSH"/>
<Product specieID="SSHpLIMK"/>


<forwardRate> 0.00877021e-3 </forwardRate>
<reverseRate> 0.332232e-3 </reverseRate>

</Reaction>


<Reaction id="pLIMK+SSH--pLIMK+SSH" name="pLIMK+SSH--SSH+LIMK">
<Reactant specieID="SSHpLIMK"/>
<Product specieID="SSH"/>
<Product specieID="LIMK"/>

<forwardRate> 0.0121175e-3 </forwardRate>
<reverseRate> 0 </reverseRate>
</Reaction>



<!--SSH activation Cof-->
<!--Cof+SSH <->SSH+pCof-->
<Reaction id="pCof+SSH--SSHpCof" name="pCof+SSH--SSHpCof">
<Reactant specieID="pCof"/>
<Reactant specieID="SSH"/>
<Product specieID="SSHpCof"/>

<!--forwardRate> 0</forwardRate-->
<forwardRate> 3.69177e-08 </forwardRate>
<reverseRate> 0.000121177e-3 </reverseRate>

</Reaction>


<Reaction id="pCof+SSH--pCof+SSH" name="pCof+SSH--SSH+Cof">
<Reactant specieID="SSHpCof"/>
<Product specieID="SSH"/>
<Product specieID="Cof"/>


<forwardRate> 3.08117e-08 </forwardRate>
<reverseRate> 0 </reverseRate>
</Reaction>



</ReactionScheme>
